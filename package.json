{
  "name": "vmate",
  "displayName": "vmate",
  "description": "vscode extension for vmate",
  "version": "0.1.0",
  "publisher": "jeffincn",
  "homepage": "https://github.com/jeffincn/vscode-vmate",
  "repository": {
    "type": "git",
    "url": "git@github.com:jeffincn/vscode-vmate.git"
  },
  "engines": {
    "vscode": "^1.17.0"
  },
  "icon": "resources/vmate.png",
  "categories": [
    "Languages",
    "Debuggers",
    "Snippets"
  ],
  "activationEvents": [
    "*"
  ],
  "main": "./out/src/extension",
  "contributes": {
    "configuration": {
      "type": "object",
      "title": "vmate options",
      "properties": {
        "vmate.author":{
          "type": [
            "string",
            "null"
          ],
          "description": "Additional author names"
        },
        "vmate.authoremail":{
          "type": [
            "string",
            "null"
          ],
          "description": "Additional author email"
        },
        "vmate.snippet.fnStyle": {
          "type": [
            "string",
            "null"
          ],
          "enum": [
            "*",
            "async"
          ],
          "default": null,
          "description": "Snippet function style"
        }
      }
    },
    "snippets": [
      {
        "language": "javascript",
        "path": "./snippets/vmate.json"
      }
    ]
  },
  "scripts": {
    "vspub": "vsce publish",
    "vscode:prepublish": "tsc -p ./",
    "compile": "tsc -watch -p ./",
    "watch": "tsc -watch -p ./",
    "postinstall": "node ./node_modules/vscode/bin/install",
    "test": "node ./node_modules/vscode/bin/test"
  },
  "dependencies": {
    "common-tags": "^1.4.0",
    "egg-utils": "^2.3.0",
    "globby": "^6.1.0",
    "is-type-of": "^1.2.0",
    "lodash.get": "^4.4.2",
    "moment": "^2.20.1",
    "mz": "^2.6.0",
    "semver": "^5.4.1"
  },
  "devDependencies": {
    "@types/mocha": "^2.2.32",
    "@types/node": "^6.0.40",
    "autod": "^2.10.1",
    "mocha": "^2.3.3",
    "typescript": "^2.0.3",
    "vscode": "^1.0.0",
    "vsce": "^1.31.1"
  },
  "license": "MIT"
}
